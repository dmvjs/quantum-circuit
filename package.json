    "qobj",
    "quil",
    "pyquil",
    "cirq",
    "qsharp"
  ],
  "author": "Quantastica <info@quantastica.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quantastica/quantum-circuit/issues"
  },
  "homepage": "https://quantum-circuit.com",
  "dependencies": {
    "antlr4": "4.7.2",
    "mathjs": "^6.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "mocha": "^6.2.3",
    "url": "^0.11.0",
    "vite": "^5.3.1"
  }
